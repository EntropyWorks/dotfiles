if [ -e  /Users/yazz/google-cloud-sdk/path.bash.inc ] ; then
  source /Users/yazz/google-cloud-sdk/path.bash.inc
  # The next line enables bash completion for gcloud.
  if [ -e  /Users/yazz/google-cloud-sdk/completion.bash.inc ] ; then
    source /Users/yazz/google-cloud-sdk/completion.bash.inc
  fi
fi

if [ -e /Users/yazz/.nix-profile/etc/profile.d/nix.sh ]; then 
  source /Users/yazz/.nix-profile/etc/profile.d/nix.sh
fi # added by Nix installer

